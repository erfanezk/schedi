{
  "name": "@taskwave/react",
  "version": "0.0.0",
  "description": "React bindings for taskwave",
  "keywords": [
    "scheduler",
    "task-scheduler",
    "interval",
    "timeout",
    "timing",
    "react",
    "hooks",
    "typescript"
  ],
  "author": "erfanezk",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/erfanezk/taskwave.git",
    "directory": "packages/react"
  },
  "homepage": "https://github.com/erfanezk/taskwave#readme",
  "bugs": {
    "url": "https://github.com/erfanezk/taskwave/issues"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "publish:npm": "pnpm publish --access public"
  },
  "dependencies": {
    "@taskwave/core": "workspace:*"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "react": "^18.2.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.7.0"
  }
}

